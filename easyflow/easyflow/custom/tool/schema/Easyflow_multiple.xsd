<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<xs:schema 
xmlns:xs="http://www.w3.org/2001/XMLSchema" 
targetNamespace="urn:de.thiema.easyflow/tools" 
xmlns="urn:de.thiema.easyflow/tools" elementFormDefault="qualified"
xmlns:types="urn:de.thiema.easyflow/types">

<xs:import schemaLocation="Easyflow.xsd" 
           namespace="urn:de.thiema.easyflow/types"/>
<xs:element name="tools">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="tool" maxOccurs="unbounded" type="types:Tool"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>
</xs:schema>
