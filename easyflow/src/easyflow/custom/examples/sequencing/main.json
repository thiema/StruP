	"#workflowTemplateFile"       : "abstract_simple1.workflow",
	"#workflowTemplateFile"       : "abstract_simple2.workflow",
	"#metadataFile"               : "metadata_abstract_simple1.tsv",
	"#metadataFile"               : "metadata_abstract_simple2.tsv",
    "#metadataFile"               : "metadata_abstract.tsv", 
    
    "isContrast"                 : "true",
    "useIntervalList"            : "true",
    "splitByContig"              : "true",
},
  "processing" : {
    "_target_enum"     : ["sge", "local", "aws", "pegasus"],
    "execution_system" : "makeflow",
    "execution_system_output_file" : "makeflow.out",
    "mem"              : "1G",
    "target"           : "sge",
    "use_threads"       : "true",
    "num_threads"       : 10,
    "queue"            : "bioinf.q",
    "handles"          : "pipe,file",
    "default_handle"    : "file",
    "stdin_operator"   : "<",
    "stdout_operator"  : "1>",
    "stderr_operator"  : "2>",
    "append_stdout_operator"  : "1>>",
    "append_stderr_operator"  : "2>>"
  },

  "catalog":{
	"reference"        : "human.fa",
	"input_dir"        : "input_dir",
	"work_dir"         : "analysis_dir",
	"tmp_dir"          : "tmp",
	},

  "contrast": {
    "_rule_enum"       : ["crossProduct", "groupWise", "sampleWise"],
    "rule"             : "crossProduct"
  },

  "workflow": {
    "metadata_input_field"    : ["InputFiles"],
    "inputs"                  : ["FASTQ"],
    "static_inputs"           : ["FASTA"],
    "inputs_map"              : {"reads" : "FASTQ"
                                },
    "static_inputs_map"       : {"reference" : "FASTA"
                                },
    "workflows"               : ["w1", "w2"], 
    "w1"                      : ["MapReads", "AddReadGroup"],
    "w2"                      : ["SampleReads", "MapReads", "AddReadgroup", "Bamprocessing", "VariantDetection"],
    "defaultGroupingCriteria" : ["Record"],
    "defaultMode"             : "batch"
  },
  
  "tool" : {
    "command_pattern" : "interpreter executable submodule positional_arg optional_arg input output",
  	"dir"             : "tool_definitions",
  	"tools_test"      : ["tool.xml", "tools.xml", "depth_of_coverage_gatk2_easyflow.xml", 
  				         "depth_of_coverage_gatk_easyflow.xml", "ngs_tools_easyflow.xml"],
  	"tools"           : ["ngs_tools_easyflow.xml", "gatk2/unified_genotyper.xml", "gatk2/indel_realigner.xml", 
  						 "pavgi_picard/picard_AddOrReplaceReadGroups.xml"],
  	"tools_"          : ["ngs_tools_easyflow.xml",
  				         "pavgi_picard", "gatk2", "bedtools-galaxy"],
  	"schemata"        : ["../../../tool/schema/Easyflow.xsd", 
  					     "../../../tool/schema/Easyflow_multiple.xsd", 
  					     "../../../tool/schema/external/Galaxy.xsd"]
  }

